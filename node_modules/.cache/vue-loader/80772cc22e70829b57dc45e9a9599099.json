ver/01_Intro/client/src/components","sourcesContent":["<template>\n  <div>\n    <h1>Konzert Tickets</h1>\n    <table>\n      <thead>\n      <tr>\n        <th>Name</th>\n        <td>&nbsp;&nbsp;&nbsp;</td>\n        <th>Preis</th>\n        <th>&nbsp;</th>\n      </tr>\n        </thead>\n        <tbody>\n        <tr v-for=\"(product, idx) in products\" :key=\"idx\">\n          <td>{{ product.name }}</td>\n          <td>&nbsp;&nbsp;&nbsp;</td>\n          <td>{{ product.price.toFixed(2) }}</td>\n          <td>&nbsp;&nbsp;&nbsp;</td>\n          <td>\n            <b-button variant=\"success\"  @click=\"addToCart(product)\">+</b-button>\n            <b-button variant=\"danger\" @click=\"removeFromCart(product)\">-</b-button>\n          </td>\n        </tr>\n        </tbody>\n      </table>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex';\nexport default {\n  name: 'Products',\n  computed: mapState(['products']),\n  methods: {\n    addToCart(product) {\n      this.$store.commit('addToCart', product);\n    },\n    removeFromCart(product) {\n      this.$store.commit('removeFromCart', product);\n    }\n  },\n  created() {\n    this.$store.dispatch(\"loadProducts\")\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\nbody {\n  font-family: 'Gadugi', Helvetica, Arial, sans-serif;\n  margin: 10%;\n  background-color: #F7F7F7;\n}\nh1 {\n  color: #00b4d8;\n}\n\nh2 {\n  font-weight: normal;\n  color: #494949;\n}\n\nth {\n  font-weight: bold;\n  color: #494949;\n}\n</style>\n"]}]}